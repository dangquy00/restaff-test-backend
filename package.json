    "google-auth-library": "^4.2.2",
    "googleapis": "^40.0.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongodb": "^3.2.7",
    "mysql": "^2.17.1",
    "node-uuid": "^1.4.8",
    "passport": "^0.4.0",
    "ts-json-properties": "2.0.3",
    "typeorm": "^0.2.25",
    "youtube-api-v3-search": "^1.2.1"
